<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup>
    <None Include="..\..\lib\*.acs" PackagePath="dotFiles" Pack="True">
      <Visible>False</Visible>
      <Pack>True</Pack>
      <PackagePath>dotFiles</PackagePath>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <None Include="DoubleAgent.x86.sxs.manifest">
      <Visible>False</Visible>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Pack>True</Pack>
      <PackagePath>dotFiles\DoubleAgent.x86\DoubleAgent.sxs.manifest</PackagePath>
      <Link>DoubleAgent.x86\DoubleAgent.sxs.manifest</Link>
    </None>
    <None Include="..\..\lib\x86\$(ConfigurationName)\DaControl.*">
      <Visible>False</Visible>
      <Pack>True</Pack>
      <PackagePath>dotFiles\DoubleAgent.x86</PackagePath>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Link>DoubleAgent.x86\%(Filename)%(Extension)</Link>
    </None>
    <None Include="..\..\lib\x86\$(ConfigurationName)\DaCore.*">
      <Visible>False</Visible>
      <Pack>True</Pack>
      <PackagePath>dotFiles\DoubleAgent.x86</PackagePath>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Link>DoubleAgent.x86\%(Filename)%(Extension)</Link>
    </None>
  </ItemGroup>

  <ItemGroup>
    <None Include="DoubleAgent.x64.sxs.manifest">
      <Visible>False</Visible>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Pack>True</Pack>
      <PackagePath>dotFiles\DoubleAgent.x64\DoubleAgent.sxs.manifest</PackagePath>
      <Link>DoubleAgent.x64\DoubleAgent.sxs.manifest</Link>
    </None>
    <None Include="..\..\lib\x64\$(ConfigurationName)\DaControl.*">
      <Visible>False</Visible>
      <Pack>True</Pack>
      <PackagePath>dotFiles\DoubleAgent.x64</PackagePath>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Link>DoubleAgent.x64\%(Filename)%(Extension)</Link>
    </None>
    <None Include="..\..\lib\x64\$(ConfigurationName)\DaCore.*">
      <Visible>False</Visible>
      <Pack>True</Pack>
      <PackagePath>dotFiles\DoubleAgent.x64</PackagePath>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Link>DoubleAgent.x64\%(Filename)%(Extension)</Link>
    </None>
  </ItemGroup>
</Project>
